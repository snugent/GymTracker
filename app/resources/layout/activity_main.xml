<?xml version="1.0" encoding="utf-8"?>
<android.widget.RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.admin1.gymtracker.activities.MainActivity">


    <Button
        android:id="@+id/btnSignOut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sign Out"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="45dp" />

    <Button
        android:id="@+id/btnProfile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/profile"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="100dp"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/btnObjective" />

    <Button
        android:id="@+id/btnExercise"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/exercise"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/btnProfile"
        android:layout_marginStart="24dp" />

    <Button
        android:id="@+id/btnObjective"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/btnSignOut"
        android:layout_marginTop="12dp"
        android:text="@string/objective" />

    <Button
        android:id="@+id/btnOtherMember"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/edit_member"
        android:layout_alignBaseline="@+id/btnObjective"
        android:layout_alignBottom="@+id/btnObjective"
        android:layout_alignStart="@+id/btnProfile"
        android:layout_marginStart="21dp" />
</android.widget.RelativeLayout>
